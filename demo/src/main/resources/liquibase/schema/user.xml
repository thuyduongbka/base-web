<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <changeSet id="create_table_user" author="duongntt">
        <createTable tableName="user">
            <column name="id" type="int(11)" autoIncrement="true">
                <constraints primaryKey="true"></constraints>
            </column>

            <column name="email" type="varchar(50)"></column>
            <column name="password" type="varchar(50)"></column>

        </createTable>
        <modifySql dbms="mysql">
            <append value="ENGINE=INNODB CHARSET=utf8mb4 COLLATE utf8mb4_unicode_520_ci"/>
        </modifySql>
    </changeSet>

</databaseChangeLog>